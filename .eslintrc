{
  "extends": [
    "airbnb",
    "airbnb/hooks",
    "airbnb-typescript",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json",
    "createDefaultProgram": true
  },
  "rules": {
    "prettier/prettier":[
      "error",{
        "endOfLine":"auto"
      }
    ],
    "@typescript-eslint/no-var-requires": "error"
  }
 
}
